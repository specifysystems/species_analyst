{
	"Version": "2012-10-17",
	"Statement": [
        {
            "Sid": "CreateRunEc2",
            "Effect": "Allow",
            "Action": [
                "ec2:DescribeInstances",
                "ec2:TerminateInstances",
                "ec2:StartInstances",
                "ec2:CreateTags",
                "ec2:RunInstances",
                "ec2:StopInstances",
                "ec2:DescribeLaunchTemplateVersions"
            ],
            "Resource": "*"
        }
	]
}
